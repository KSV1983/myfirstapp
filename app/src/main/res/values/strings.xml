<resources>
    <string name="app_name">My First Application</string>
    <string name="text_view_1">Была та смутная пора,</string>
    <string name="text_view_2">Когда Россия молодая,</string>
    <string name="text_view_3">В бореньях силы напрягая, </string>
    <string name="text_view_4">Мужала с гением Петра.</string>
